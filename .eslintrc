{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "no-console": 0,
        "react/destructuring-assignment": 0,
        "react/sort-comp": 0,
        "class-methods-use-this": 0,
        "no-use-before-define": 0,
        "no-shadow": 0,
        "prefer-destructuring": 0,
        "react/prop-types": 0,
        "global-require": 0,
        "import/prefer-default-export": 0
    },
    "env": {
        "browser": true,
        "node": true
    }
}
